{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>int128/renovate-base",
    "github>int128/go-renovate-config#v1.9.0",
    "github>int128/go-renovate-config:kubebuilder#v1.9.0",
    "github>int128/go-renovate-config:go-version-with-dockerfile#v1.9.0",
    "github>int128/go-renovate-config:github-releases#v1.9.0(README.md)",
    "github>int128/go-renovate-config:github-releases#v1.9.0(**/kustomization.yaml)",
    "github>aquaproj/aqua-renovate-config#2.10.0",
    "helpers:pinGitHubActionDigests"
  ],
  "packageRules": [
    {
      "description": "Ignore replaced Go packages in github.com/argoproj/argo-cd/v2",
      "matchPackageNames": [
        "github.com/argoproj/gitops-engine"
      ],
      "matchPackagePrefixes": [
        "k8s.io/"
      ],
      "enabled": false
    },
    {
      "matchPackagePrefixes": [
        "github.com/google/go-github/v"
      ],
      "matchUpdateTypes": [
        "major"
      ],
      "automerge": true
    }
  ],
  "platformAutomerge": true
}
